<h2>Bảng cửu chương từ 2 đến 9:</h2>

@for (int i = 2; i <= 9; i++)
{
    <h3>Bảng cửu chương @i:</h3>
    <table border="1">
        @for (int j = 1; j <= 10; j++)
        {
            <tr>
                <td>@i</td>
                <td>x</td>
                <td>@j</td>
                <td>=</td>
                <td>@(i * j)</td>
            </tr>
        }
    </table>
}
